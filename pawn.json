{
    "user": "Shiska",
    "repo": "rotations",
    "entry": "test.pwn",
    "output": "test.amx",
    "dependencies": [
        "sampctl/samp-stdlib"
    ],
    "dev_dependencies": [
        "pawn-lang/YSI-Includes",
		"samp-incognito/samp-streamer-plugin"
    ],
    "builds": [
        {
            "name": "amx",
            "args": [
                "-;+", "-(+", "-d3"
            ]
        },
        {
            "name": "compat"
        },
        {
            "name": "lst",
            "args": [
                "-l"
            ]
        },
        {
            "name": "asm",
            "args": [
                "-a"
            ]
        },
        {
            "name": "report",
            "args": [
                "-r"
            ],
            "input": "rotation.inc"
        },
        {
            "name": "report_misc",
            "args": [
                "-r"
            ],
            "input": "rotation_misc.inc"
        },
        {
            "name": "report_extra",
            "args": [
                "-r"
            ],
            "input": "rotation_extra.inc"
        }
    ],
    "runtime": {
        "mode": "y_testing"
    }
}